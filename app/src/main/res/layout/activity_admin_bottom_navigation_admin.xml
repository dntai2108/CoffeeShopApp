<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.Admin_Bottom_Navigation_Admin">

    <FrameLayout
        android:id="@+id/framelayout"

        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/nav_static_admin"

        tools:layout_editor_absoluteY="64dp" />
    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/nav_static_admin"
        android:layout_width="match_parent"
        app:labelVisibilityMode="labeled"
        app:itemIconTint="@color/custom_bottom_nav_color"
        app:itemTextColor="@color/custom_bottom_nav_color"
        android:layout_height="wrap_content"
        android:layout_marginStart="0dp"
        android:layout_marginEnd="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_alignParentBottom="true"
        android:background="?android:attr/windowBackground"
        app:menu="@menu/menu_static_admin" />
</RelativeLayout>